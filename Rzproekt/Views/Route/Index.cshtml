@{
    Layout = "_Layout";
}
<div class="block main-block">
    <section class="main-block-title">
        <h1>{{ header[0].mainTitle }}</h1>
        <a href="#"><span class="arrow-down"></span></a>
    </section>
</div>

<div class="block service">
    <div class="title-block">
        <h1>{{ service[0].mainTitle }}</h1>
    </div>
    <div class="border-bottom-title"></div>
    <section class="service-list">
        <article v-for="item in service">
            <img :src="item.url" alt="service" />
            <h1>{{ item.title }}</h1>
            <p>{{ item.text }}</p>
        </article>
    </section>
    <div class="btn-detail">
        <a href="#">{{ service[0].buttonText }}</a>
    </div>
    
</div>

<div class="block about">
    <div class="about-left">
        <div class="about-block-txt">
            <div class="title-block">
                <h1>{{ about[0].mainTitle }}</h1>
            </div>
            <div class="border-bottom-title"></div>
            <div class="about-txt">
                <p>{{ about[0].text }}</p>
            </div>
            <div class="btn-detail">
                <a href="#">{{ about[0].buttonText }}</a>
            </div>
        </div>
    </div>
    <div class="about-right"></div>

</div>
<div class="block-counter">
    <div class="counter">
        <div class="numeral">{{ stat[0].number }}</div>
        <div class="numeral-detal"><h2>{{ stat[0].text }}</h2></div>
    </div>
    <div class="block-border-counter"></div>
    <div class="counter">
        <div class="numeral">{{ stat[1].number }}</div>
        <div class="numeral-detal"><h2>{{ stat[1].text }}</h2></div>
    </div>
    <div class="block-border-counter"></div>
    <div class="counter">
        <div class="numeral">{{ stat[2].number }}</div>
        <div class="numeral-detal"><h2>{{ stat[2].text }}</h2></div>
    </div>
    <div class="block-border-counter"></div>
    <div class="counter">
        <div class="numeral">{{ stat[3].number }}</div>
        <div class="numeral-detal"><h2>{{ stat[3].text }}</h2></div>
    </div>
</div>
<div class="block project">
    <div class="title-block">
        <h1>{{ project[0].mainTitle }}</h1>
    </div>
    <div class="border-bottom-title"></div>
    <section class="project-list">
        <article v-for="item in project">
            <div :src="item.url" class="project-block">
                <h1>{{ item.title }}</h1>
            </div>
        </article>
    </section>
    <div class="btn-detail">
        <a href="#">{{ project[0].buttonText }}</a>
    </div>
</div>

<div class="block clients">
    <div class="title-block">
        <h1>{{ client[0].mainTitle }}</h1>
    </div>
    <div class="border-bottom-title"></div>
    <section class="clients-list">

        <article class="clients-block" v-for="item in client">
            <img :src="item.url" />
        </article>

    </section>
    <div class="btn-detail">
        <a href="#">{{ client[0].buttonText }}</a>
    </div>
</div>
<div class="block contacts">
    <div class="title-block">
        <h1>{{ contact[0].mainTitle }}</h1>
    </div>
    <div class="border-bottom-title"></div>
    <div class="map-block">
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2135.476580426758!2d39.87244051610277!3d57.64038515230752!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x46b2916fd18e267d%3A0x65d18492882ca52!2z0KHQvtCy0LXRgtGB0LrQsNGPINGD0LsuLCA2OSwg0K_RgNC-0YHQu9Cw0LLQu9GMLCDQr9GA0L7RgdC70LDQstGB0LrQsNGPINC-0LHQuy4sIDE1MDAwMw!5e0!3m2!1sru!2sru!4v1602061981456!5m2!1sru!2sru" width="800" height="300" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
    </div>
    <section class="contacts-list">
        <article class="contacts-block">
            <div class="address">
                <h1>{{ contact[0].title }}</h1><br /> 
                <p>{{ contact[1].text }} <br /><br /> {{ contact[2].text }} <br /><br /> {{ contact[3].text }}</p>
            </div>
            <div class="detail-contacts">
                <div class="btn-detail">
                    <a href="#">{{ contact[0].buttonText }}</a>
                </div>
            </div>
        </article>
    </section>
</div>